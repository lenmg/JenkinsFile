pipeline {	 
	agent {
  		label {
        		label "slave1"
  }
}
	tools {
		maven "my_mvn"
	}
    	stages {     	 
    	stage("Compile") {          	 
            	steps {               	 
                	sh "mvn compile"          	 
            	}     	 
        	}     
		}
  agent {
  		label {
        		label "slave2"
  }
}
	stages {
    	stage("Unit test") {          	 
        	steps {               	 
                	sh "mvn test"          	 
            	}     	 
        	}	 
    	}
}
